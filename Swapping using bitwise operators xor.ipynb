   "source": [
    "#swapping using bitwise operators xor\n",
    "a=int(input(\"Enter the value of a:\"))\n",
    "b=int(input(\"Enter the value of b:\"))\n",
    "print(\"Before swapping a=\",a,\"b=\",b)\n",
    "a=a^b\n",
    "b=a^b\n",
    "a=a^b\n",
    "print(\"after swapping a= \",round(a),\"b= \",round(b))"
   ]
  }
